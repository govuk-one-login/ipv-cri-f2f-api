{
  "name": "F2F",
  "version": "1.0.0",
  "description": "OneLogin F2F CRI",
  "repository": "https://github.com/alphagov/di-ipv-cri-f2f-api",
  "author": "GDS",
  "license": "MIT",
  "dependencies": {
    "@aws-lambda-powertools/logger": "1.5.1",
    "@aws-lambda-powertools/metrics": "1.5.1",
    "@aws-sdk/client-dynamodb": "3.930.0",
    "@aws-sdk/client-kms": "3.930.0",
    "@aws-sdk/client-lambda": "3.930.0",
    "@aws-sdk/client-s3": "3.930.0",
    "@aws-sdk/client-sfn": "3.930.0",
    "@aws-sdk/client-sqs": "3.930.0",
    "@aws-sdk/client-ssm": "3.930.0",
    "@aws-sdk/credential-providers": "3.930.0",
    "@aws-sdk/lib-dynamodb": "3.930.0",
    "@smithy/node-http-handler": "4.0.4",
    "@types/node-jose": "1.1.13",
    "aws-xray-sdk-core": "3.10.3",
    "axios": "1.8.4",
    "body-parser": "2.2.0",
    "class-validator": "0.14.1",
    "dotenv-cli": "7.4.4",
    "ecdsa-sig-formatter": "1.0.11",
    "esbuild": "0.25.9",
    "jose": "5.10.0",
    "jsonpath": "1.1.1",
    "node-jose": "2.2.0",
    "node-rsa": "1.1.1",
    "notifications-node-client": "7.0.6",
    "npm-run-all": "4.1.5",
    "pdf-lib": "1.17.1",
    "pdf-merger-js": "5.1.2",
    "pdfkit": "0.16.0"
  },
  "scripts": {
    "unit": "node --experimental-vm-modules ./node_modules/.bin/jest --testPathPattern=tests/unit/ --coverage",
    "test:unit": "npm run compile && npm run unit",
    "lint:fix": "eslint --fix --output-file ./reports/eslint/report.html --format html -c eslint.config.mjs --ext *.ts .",
    "compile": "./node_modules/.bin/tsc",
    "lint": "eslint -c eslint.config.mjs *.ts .",
    "test:e2e": "node --experimental-vm-modules ./node_modules/.bin/jest --testMatch '**/e2e/?(*.)test.ts' --passWithNoTests",
    "test:infra": "node --experimental-vm-modules ./node_modules/.bin/jest --testMatch '**/infra/?(*.)test.ts' ",
    "api": "node --experimental-vm-modules JEST_JUNIT_OUTPUT_NAME=api-report.xml ./node_modules/.bin/jest --runInBand --testPathPattern=tests/api/backend",
    "api-traffic": "node --experimental-vm-modules JEST_JUNIT_OUTPUT_NAME=api-report.xml ./node_modules/.bin/jest --runInBand --testPathPattern='tests/api/backend/(HappyPath|CallbackApi)\\.test\\.ts$'",
    "test:api": "npm run compile && npm run api",
    "test:api-traffic": "npm run compile && npm run api-traffic",
    "api-third-party": "node --experimental-vm-modules THIRD_PARTY_CLIENT_ID=SandboxJourneyFlow JEST_JUNIT_OUTPUT_NAME=third-party-api-report.xml ./node_modules/.bin/jest --runInBand --testMatch '**/tests/api/backend/**/*Path.test.ts'",
    "test:api-third-party": "npm run compile && npm run api-third-party",
    "api-third-party-traffic": "node --experimental-vm-modules THIRD_PARTY_CLIENT_ID=SandboxJourneyFlow JEST_JUNIT_OUTPUT_NAME=third-party-api-report.xml ./node_modules/.bin/jest --runInBand --testPathPattern=tests/api/backend/HappyPath.test.ts",
    "test:api-third-party-traffic": "npm run compile && npm run api-third-party-traffic",
    "yoti": "node --experimental-vm-modules JEST_JUNIT_OUTPUT_NAME=api-report.xml ./node_modules/.bin/jest --runInBand --testPathPattern=tests/api/stub/YotiStub.test.ts",
    "test:yoti": "npm run compile && npm run yoti",
    "postoffice": "node --experimental-vm-modules JEST_JUNIT_OUTPUT_NAME=api-report.xml ./node_modules/.bin/jest --runInBand --testPathPattern=tests/api/stub/PostOfficeStub.test.ts",
    "test:postoffice": "npm run compile && npm run postoffice",
    "govnotify": "node --experimental-vm-modules JEST_JUNIT_OUTPUT_NAME=api-report.xml ./node_modules/.bin/jest --runInBand --testPathPattern=tests/api/stub/GovNotifyStub.test.ts",
    "test:govnotify": "npm run compile && npm run govnotify",
    "test:pii": "bash ./check-logs.sh",
    "kill:dynamodblocal": "cd tests/contract; ./scripts/kill-dynamodb-local.sh",
    "start:dynamodblocal": "cd tests/contract; ./scripts/setup-dynamodb-local.sh",
    "test:pactconnection": "cd tests/contract; ./scripts/test-pact-connection.sh",
    "start:contract": "npm-run-all -s start:dynamodblocal test:pactconnection wait-start:contract",
    "wait-start:contract": "wait-on tcp:127.0.0.1:8000 && cd tests/contract && ts-node ./app.ts",
    "test:provider": "node --experimental-vm-modules ./node_modules/.bin/jest --testMatch '**/contract/F2fCriProvider.test.ts' --coverage=false",
    "test:contract": "wait-on tcp:127.0.0.1:3000; cd tests/contract; npm run test:provider && npm run kill:dynamodblocal",
    "test:contract:ci": "dotenv -e.env.contract -- npm-run-all -p -r start:contract test:contract",
    "deploy": "cd ../deploy ; sam build && sam deploy --config-env dev --resolve-s3 ; cd src/"
  },
  "devDependencies": {
    "@babel/preset-env": "7.26.9",
    "@eslint/compat": "1.2.7",
    "@pact-foundation/pact": "12.1.3",
    "@types/aws-lambda": "8.10.148",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.14",
    "@types/jest-image-snapshot": "6.4.0",
    "@types/js-yaml": "4.0.9",
    "@types/jsonpath": "0.2.4",
    "@types/node": "20.14.8",
    "@types/node-rsa": "1.1.4",
    "@types/pdfkit": "0.13.8",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "ajv": "8.17.1",
    "aws-cdk-lib": "2.196.0",
    "aws4-axios": "3.4.0",
    "canvas": "3.1.0",
    "dotenv": "16.4.7",
    "eslint": "9.31.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-jsdoc": "50.6.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-security": "1.6.0",
    "express": "4.21.2",
    "express-asyncify": "2.1.2",
    "fast-xml-parser": "4.2.7",
    "jest": "29.7.0",
    "jest-html-reporter": "3.10.2",
    "jest-image-snapshot": "6.4.0",
    "jest-junit": "16.0.0",
    "jest-mock-extended": "3.0.7",
    "pdfjs-dist": "2.16.105",
    "prettier": "3.6.2",
    "ts-jest": "29.3.1",
    "ts-node": "10.9.2",
    "ts-standard": "12.0.2",
    "tslint": "6.1.3",
    "typescript": "4.9.5",
    "wait-on": "7.2.0",
    "yaml-cfn": "0.3.2"
  },
  "engines": {
    "node": "22.*"
  },
  "overrides": {
    "form-data": ">=4.0.4"
  }
}
