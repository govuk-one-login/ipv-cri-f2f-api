{
  "name": "F2F",
  "version": "1.0.0",
  "description": "OneLogin F2F CRI",
  "repository": "https://github.com/alphagov/di-ipv-cri-f2f-api",
  "author": "GDS",
  "license": "MIT",
  "dependencies": {
    "@aws-lambda-powertools/logger": "1.5.1",
    "@aws-lambda-powertools/metrics": "1.5.1",
    "@aws-sdk/client-dynamodb": "3.726.1",
    "@aws-sdk/client-kms": "3.726.1",
    "@aws-sdk/client-lambda": "3.726.1",
    "@aws-sdk/client-s3": "3.729.0",
    "@aws-sdk/client-sfn": "3.726.1",
    "@aws-sdk/client-sqs": "3.726.1",
    "@aws-sdk/client-ssm": "3.726.1",
    "@aws-sdk/credential-providers": "3.729.0",
    "@aws-sdk/lib-dynamodb": "3.726.1",
    "@smithy/node-http-handler": "3.3.1",
    "@pact-foundation/pact": "^12.1.3",
    "aws-xray-sdk-core": "^3.4.1",
    "axios": ">=1.7.9",
    "body-parser": "^1.20.2",
    "class-validator": "0.14.1",
    "dotenv-cli": "^7.3.0",
    "ecdsa-sig-formatter": "^1.0.11",
    "esbuild": "0.14.14",
    "express": "^4.18.2",
    "express-asyncify": "^2.1.2",
    "jose": "^5.2.1",
    "lodash": "^4.17.21",
    "node-jose": "^2.1.1",
    "node-rsa": "^1.1.1",
    "notifications-node-client": "^7.0.0",
    "npm-run-all": "^4.1.5"
  },
  "scripts": {
    "unit": "./node_modules/.bin/jest --testPathPattern=tests/unit --coverage",
    "test:unit": "npm run compile && npm run unit",
    "lint:fix": "eslint --fix --output-file ./reports/eslint/report.html --format html -c .eslintrc.js --ext .ts .",
    "compile": "./node_modules/.bin/tsc",
    "lint": "eslint --ext .ts .",
    "test:e2e": "./node_modules/.bin/jest --testMatch '**/e2e/?(*.)test.ts' --passWithNoTests",
    "test:infra": "./node_modules/.bin/jest --testMatch '**/infra/?(*.)test.ts' ",
    "api": "JEST_JUNIT_OUTPUT_NAME=api-report.xml ./node_modules/.bin/jest --runInBand --testPathPattern=tests/api/backend",
    "api-traffic": "JEST_JUNIT_OUTPUT_NAME=api-report.xml ./node_modules/.bin/jest --runInBand --testPathPattern='tests/api/backend/(HappyPath|CallbackApi)\\.test\\.ts$'",
    "test:api": "npm run compile && npm run api",
    "test:api-traffic": "npm run compile && npm run api-traffic",
    "api-third-party": "THIRD_PARTY_CLIENT_ID=SandboxJourneyFlow JEST_JUNIT_OUTPUT_NAME=third-party-api-report.xml ./node_modules/.bin/jest --testMatch '**/tests/api/backend/**/*Path.test.ts'",
    "test:api-third-party": "npm run compile && npm run api-third-party",
    "api-third-party-traffic": "THIRD_PARTY_CLIENT_ID=SandboxJourneyFlow JEST_JUNIT_OUTPUT_NAME=third-party-api-report.xml ./node_modules/.bin/jest --testPathPattern=tests/api/backend/HappyPath.test.ts",
    "test:api-third-party-traffic": "npm run compile && npm run api-third-party-traffic",
    "yoti": "JEST_JUNIT_OUTPUT_NAME=api-report.xml ./node_modules/.bin/jest --runInBand --testPathPattern=tests/api/stub/YotiStub.test.ts",
    "test:yoti": "npm run compile && npm run yoti",
    "postoffice": "JEST_JUNIT_OUTPUT_NAME=api-report.xml ./node_modules/.bin/jest --runInBand --testPathPattern=tests/api/stub/PostOfficeStub.test.ts",
    "test:postoffice": "npm run compile && npm run postoffice",
    "govnotify": "JEST_JUNIT_OUTPUT_NAME=api-report.xml ./node_modules/.bin/jest --runInBand --testPathPattern=tests/api/stub/GovNotifyStub.test.ts",
    "test:govnotify": "npm run compile && npm run govnotify",
    "test:pii": "bash ./check-logs.sh",
    "kill:dynamodblocal": "cd tests/contract; ./scripts/kill-dynamodb-local.sh",
    "start:dynamodblocal": "cd tests/contract; ./scripts/setup-dynamodb-local.sh",
    "test:pactconnection": "cd tests/contract; ./scripts/test-pact-connection.sh",
    "start:contract": "npm-run-all -s start:dynamodblocal test:pactconnection wait-start:contract",
    "wait-start:contract": "wait-on tcp:127.0.0.1:8000 && cd tests/contract && ts-node ./app.ts",
    "test:provider": "./node_modules/.bin/jest --testMatch '**/contract/F2fCriProvider.test.ts' --coverage=false",
    "test:contract": "wait-on tcp:127.0.0.1:3000; cd tests/contract; npm run test:provider && npm run kill:dynamodblocal",
    "test:contract:ci": "dotenv -e.env.contract -- npm-run-all -p -r start:contract test:contract",
    "deploy": "cd ../deploy ; sam build && sam deploy --config-env dev --resolve-s3 ; cd src/"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.109",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.4.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^18.11.18",
    "@types/node-jose": "^1.1.10",
    "@types/node-rsa": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.50.0",
    "ajv": "^8.12.0",
    "aws-cdk-lib": "^2.147.3",
    "aws4-axios": "^3.2.24",
    "dotenv": "^16.3.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsdoc": "^39.6.8",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.6.0",
    "fast-xml-parser": "^4.2.7",
    "jest": "^29.4.1",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "jest-mock-extended": "^3.0.1",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.2",
    "ts-standard": "^12.0.2",
    "tslint": "^6.1.3",
    "typescript": "^4.9.4",
    "wait-on": "^7.2.0",
    "yaml-cfn": "^0.3.2"
  },
  "engines": {
    "node": "^18.0.0"
  }
}
